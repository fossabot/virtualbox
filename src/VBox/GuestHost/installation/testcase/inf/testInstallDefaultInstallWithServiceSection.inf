[Version]
Signature="$WINDOWS NT$"
Provider=%ORACLE%
ClassGuid={4D36E97D-E325-11CE-BFC1-08002BE10318}
Class=System
DriverPackageType=PlugAndPlay

; Note: Decoration casing is intentional for testing! Don't change!

[DefaultInstall]
%ORACLE%=VBoxTest,NtAmd64

[DefaultInstall.NTAMD64.Services]
AddService=VBoxTest,0x00000002,VBoxTest_Service

[VBoxTest_Service]
DisplayName    = %VBoxTest.SVCDESC%
ServiceType    = %SERVICE_TYPE_KERNEL_DRIVER%
StartType      = %SERVICE_STARTTYPE_AUTO%
ErrorControl   = %SERVICE_ERRORCTL_NORMAL%
ServiceBinary  = %12%\VBoxTest.sys

[Strings]
ORACLE = "Oracle Corporation"
VBoxTest.SVCDESC = "VirtualBox Test Service"
VBoxTest.DRVDESC = "VirtualBox Test Driver"
VBoxTest.DSKDESC = "VirtualBox Test Driver Installation Disk"

; Useful constants
SERVICE_TYPE_KERNEL_DRIVER = 1
SERVICE_STARTTYPE_BOOT = 0
SERVICE_STARTTYPE_AUTO = 1
SERVICE_STARTTYPE_DEMAND = 3
SERVICE_ERRORCTL_IGNORE = 0
SERVICE_ERRORCTL_NORMAL = 1
